{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/api/(.*)", "dest": "/index.js" },
    { "src": "/notes", "dest": "/public/notes.html" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/assets/(.*)", "dest": "/public/assets/$1" }
  ]
}
